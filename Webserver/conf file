<VirtualHost www.itstudy.com.au>
ServerName www.itstudy.com.au
DocumentRoot "C:/itstudy"
<Directory "C:/itstudy">
    Options Indexes FollowSymLinks Includes EXECCGI
	AllowOverride ALL
    Require all granted
</Directory>
Alias /login "C:/itstudy/teacher"
<Directory "C:/itstudy/teacher">
	Options Indexes FollowSymLinks Includes EXECCGI
#	AllowOverride ALL
#    Require all granted
	AuthType Basic
	AuthUserFile "c:/password_files/basic.pwd"
	AuthName "User Login"
	Require valid-user
	Allow from all
</Directory>
Alias /student "C:/itstudy/student"
<Directory "C:/itstudy/student">
    Options Indexes FollowSymLinks Includes EXECCGI
	AllowOverride ALL
    Require all granted
</Directory>
</Virtualhost>

<VirtualHost www.studyit.com.au>
ServerName www.studyit.com.au
DocumentRoot "C:/studyit"
<Directory "C:/studyit">
    Options Indexes FollowSymLinks Includes EXECCGI
	AllowOverride ALL
    Require all granted
</Directory>
Alias /student "C:/studyit/login"
<Directory "C:/studyit/login">
	Options Indexes FollowSymLinks Includes EXECCGI
#	AllowOverride ALL
#    Require all granted
	AuthType Digest
	AuthUserFile "c:/password_files/studyit.pwd"
	AuthName "User Login"
	Require valid-user
	Allow from all
</Directory>
</Virtualhost>

